{
  "name": "cc-stack-marketplace",
  "owner": {
    "name": "jovermier",
    "url": "https://github.com/jovermier"
  },
  "metadata": {
    "description": "Claude Code marketplace for full-stack development agents covering Go, GraphQL, Next.js, and Playwright",
    "version": "1.0.0"
  },
  "plugins": [
    {
      "name": "cc-go",
      "description": "Claude Code agents and skills for Go development. Performance, concurrency, error handling, and testing expertise.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["go", "golang", "code-review", "performance", "concurrency", "error-handling", "testing", "skills"],
      "source": "./plugins/cc-go"
    },
    {
      "name": "cc-graphql",
      "description": "Claude Code agents and skills for GraphQL development. Schema design, resolvers, and mutations expertise.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["graphql", "gql", "api", "schema-design", "resolvers", "mutations", "code-review", "skills"],
      "source": "./plugins/cc-graphql"
    },
    {
      "name": "cc-nextjs",
      "description": "Claude Code agents and skills for Next.js development. Server components, metadata, and performance expertise.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["nextjs", "react", "server-components", "app-router", "metadata", "performance", "code-review", "skills"],
      "source": "./plugins/cc-nextjs"
    },
    {
      "name": "cc-playwright",
      "description": "Claude Code agents and skills for Playwright testing. Page objects, best practices, and test generation expertise.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["playwright", "e2e-testing", "testing", "page-objects", "test-generation", "skills"],
      "source": "./plugins/cc-playwright"
    },
    {
      "name": "cc-skillsmp",
      "description": "Auto-add skills from SkillsMP.com marketplace to your project. Search and install Claude skills directly using keyword or AI semantic search.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["skills", "marketplace", "skillsmp", "skill-installation", "productivity", "ai-search"],
      "source": "./plugins/cc-skillsmp",
      "commands": ["./skill-add.md", "./skill-discover.md"]
    },
    {
      "name": "auto-skills",
      "description": "Automatically detect project needs and install relevant skills from SkillsMP and other sources. Run /auto-skills to trigger.",
      "version": "1.0.0",
      "author": {
        "name": "jovermier",
        "url": "https://github.com/jovermier"
      },
      "homepage": "https://github.com/jovermier/cc-stack-marketplace",
      "repository": "https://github.com/jovermier/cc-stack-marketplace",
      "license": "MIT",
      "tags": ["auto-install", "self-improving", "automation", "productivity", "skills"],
      "source": "./plugins/auto-skills",
      "commands": ["./auto-skills.md"],
      "skills": ["./skills/context-detection", "./skills/auto-skill"]
    }
  ]
}
